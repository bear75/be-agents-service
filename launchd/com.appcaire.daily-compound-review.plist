<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.appcaire.daily-compound-review</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Users/be-agent-service/HomeCare/be-agents-service/scripts/compound/daily-compound-review.sh</string>
    <string>appcaire</string>
  </array>

  <key>WorkingDirectory</key>
  <string>/Users/be-agent-service/HomeCare/be-agents-service</string>

  <key>StartCalendarInterval</key>
  <dict>
    <key>Hour</key>
    <integer>22</integer>
    <key>Minute</key>
    <integer>30</integer>
  </dict>

  <key>StandardOutPath</key>
  <string>/Users/be-agent-service/HomeCare/appcaire/logs/compound-review.log</string>

  <key>StandardErrorPath</key>
  <string>/Users/be-agent-service/HomeCare/appcaire/logs/compound-review.log</string>

  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
  </dict>
</dict>
</plist>
